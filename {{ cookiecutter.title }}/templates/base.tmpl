{%- raw -%}
\documentclass{article}

\begin{latest}{$ packages $}[join={$ '\n' $}]
\usepackage\latest{$ '[' + opt + ']' if 'opt' in vars() else '' $}{\latest{$ pkg $}}
\end{latest}

\usetikzlibrary{\begin{latest}{$ list(map(lambda x: {'lib': x}, tikz.libs)) $}[join={$ ',' $}]\latest{$ lib $}\end{latest}}

\begin{latest}{$ [dict(ctx, context=ctx) for ctx in tikz.styles] $}
\tikzset{\latest{$ name $}/.style={\latest{$ attrs $}}}
\begin{latest}{$ 'def' in context $}
\tikzset{\latest{$ name $}/.default=\latest{$ context['def'] $}}
\end{latest}\end{latest}

\begin{latest}{$ units $}[join={$ '\n' $}]
\DeclareSIUnit\latest{$ cmd $}{\latest{$ sym $}}
\end{latest}

\newcommand{\half}{$\frac{1}{2}$}
\newcommand{\cell}[1]{%
    \begin{tabular}{@{}c@{}}#1\end{tabular}%
}

\newcolumntype{L}[1]{>{\hsize=#1\hsize\raggedright\arraybackslash}X}
\newcolumntype{R}[1]{>{\hsize=#1\hsize\raggedleft\arraybackslash}X}

\begin{latest}{$ 'extra' in vars() $}
\begin{latest}{$ [{'extra': x} for x in extra] $}[join={$ '\n' $}]
\latest{$ extra $}
\end{latest}\end{latest}

\title{\latest{$ title $}}
\author{\latest{$ author $}}
\date{\textit{\latest{$ date $}}}

{%- endraw %}


\begin{document}
\maketitle
{% block content %}
{% endblock %}
\end{document}
