{%- raw -%}
\documentclass{article}

\begin{latest}{$ packages $}[join={$ '\n' $}]
\usepackage\latest{$ '[' + opt + ']' if 'opt' in vars() else '' $}{\latest{$ pkg $}}
\end{latest}

\usetikzlibrary{\begin{latest}{$ list(map(lambda x: {'lib': x}, tikzlibs)) $}[join={$ ',' $}]\latest{$ lib $}\end{latest}}

\title{\vspace{\latest{$space$}}\latest{$ title $}}
\author{\latest{$ author $}}
\date{\textit{\latest{$ date $}}}

\pagenumbering{gobble}

\DeclareSIUnit\atm{atm}
\DeclareSIUnit\dioptre{D}

\tikzset{
    point/.style={circle,fill,black!80,inner sep=0pt,minimum size=#1,opacity=0.9},
    point/.default=3pt,
    vector/.style={line width=1pt,postaction={decorate,decoration={markings,mark=at position 1 with {\arrow{latex}}}}},
    block/.style={rectangle,fill=black!30,draw,minimum size=#1,opacity=0.9},
    block/.default=15pt,
    ball/.style={circle,fill=black!30,draw,minimum size=#1,opacity=0.9},
    ball/.default=5pt,
    pulley/.style={line width=1.5pt},
    rod/.style={line width=2pt},
    rope/.style={line width=1pt},
    spring/.style={decorate,decoration={coil,amplitude=5pt,segment length=#1,aspect=0.3}},
    spring/.default=5pt,
    wall/.style={black!10,pattern=north east lines,opacity=0.3}
}

\newcommand{\half}{$\frac{1}{2}$}
\newcommand{\cell}[1]{%
    \begin{tabular}{@{}c@{}}#1\end{tabular}%
}

\newcolumntype{L}[1]{>{\hsize=#1\hsize\raggedright\arraybackslash}X}
\newcolumntype{R}[1]{>{\hsize=#1\hsize\raggedleft\arraybackslash}X}

\begin{latest}{$ extra $}
\begin{latest}{$ list(map(lambda x: {'extra': x}, extra)) $}[join={$ '\n' $}]
\latest{$ extra $}
\end{latest}
\end{latest}
